{
	"name": "shineunited/tagmanager",
	"type": "library",
	"description": "Basic PHP container for Google Tag Manager",
	"keywords": ["google", "tag", "manager", "tag manager", "gtm"],
	"license": "MIT",
	"require": {
		"php": ">=5.4.0",
		"silex/silex": "^2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0|~5.0",
		"twig/twig": "^1.33"
	},
	"suggest": {
		"twig/twig": "Allow use of tag manager inside of twig templates"
	},
	"autoload": {
		"psr-4": {
			"ShineUnited\\TagManager\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ShineUnited\\TagManager\\Tests\\": "tests/"
		}
	}
}
